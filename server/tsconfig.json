{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": ["node"],
    "lib": ["es6"],
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react-jsx"
  }
  // "include": ["./**/*.tsx", "./**/*.jsx"]
}
